------------------------------
-- Measure: [UPN filter image]
------------------------------
MEASURE UserConfiguration[UPN filter image] = 
    VAR _UPN = USERPRINCIPALNAME()
    
    VAR _Result =
    CALCULATE(MIN('Country'[Image]),'Country'[User email] = _UPN)
    
    RETURN
    _Result

-----------------------------
-- Measure: [UPN filter name]
-----------------------------
MEASURE UserConfiguration[UPN filter name] = 
    VAR _UPN = USERPRINCIPALNAME()
    
    VAR _Result =
    IF(ISBLANK(
    CALCULATE(MIN('Country'[Country Manager]),'Country'[User email] = _UPN)
    ), USERPRINCIPALNAME(),
    CALCULATE(MIN('Country'[Country Manager]),'Country'[User email] = _UPN)
    )
    
    RETURN
    _Result

-------------------------
-- Measure: [UPN Welcome]
-------------------------
MEASURE UserConfiguration[UPN Welcome] = "Welcome " & [UPN filter name]

-----------------------------------------
-- Calculation Group: 'UserConfiguration'
-----------------------------------------
CALCULATIONGROUP UserConfiguration[User configuration]

    CALCULATIONITEM "Global" = SELECTEDMEASURE()

    CALCULATIONITEM "My stats" = 
        VAR _UPN = USERPRINCIPALNAME()
        VAR _Result =
        CALCULATE(SELECTEDMEASURE(),'Country'[User Email] = _UPN)
        
        RETURN
        _Result

